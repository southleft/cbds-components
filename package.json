{
  "name": "cbds-components",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "tokens:build": "node tokens/build.mjs",
    "tokens:watch": "concurrently \"chokidar 'tokens/source/**/*.json' -c 'npm:tokens:build'\"",
    "dev": "concurrently \"npm:tokens:build\" \"vite\"",
    "build": "npm run tokens:build && tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "npm run tokens:build && storybook build"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "clsx": "^2.1.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/js": "^9.33.0",
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-docs": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.1.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^9.1.5",
    "globals": "^16.3.0",
    "storybook": "^8.6.14",
    "style-dictionary": "^4.2.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vitest": "^3.2.4",
    "@vitest/browser": "^3.2.4",
    "playwright": "^1.55.0",
    "@vitest/coverage-v8": "^3.2.4"
  }
}

{
  "name": "cbds-components",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "tokens": "node tokens/build.mjs",
    "tokens:watch": "concurrently \"chokidar 'tokens/json/**/*.json' -c 'npm run tokens'\"",
    "dev": "concurrently \"npm run tokens\" \"vite\"",
    "build": "npm run tokens && tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "npm run tokens && storybook build"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/js": "^9.35.0",
    "@storybook/addon-a11y": "^9.1.5",
    "@storybook/addon-docs": "^9.1.5",
    "@storybook/react-vite": "^9.1.5",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^9.1.5",
    "globals": "^16.3.0",
    "playwright": "^1.55.0",
    "storybook": "^9.1.5",
    "style-dictionary": "^5.0.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.5",
    "vitest": "^3.2.4"
  }
}
